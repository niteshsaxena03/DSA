class Solution{
	public:
	int solvemem(int n,vector<int>&dp){
	    if(n==0)return 0;
	    if(dp[n]!=-1)return dp[n];
	    int ans=n;
	    for(int i=1;i*i<=n;i++){
	        int temp=i*i;
	        
	        ans=min(ans,1+solvemem(n-temp,dp));
	    }
	    return dp[n]=ans;
	}
	
	int MinSquares(int n)
	{
	    vector<int>dp(n+1,-1);
	    return solvemem(n,dp);
	}
};
